{
  "name": "clanpress",
  "version": "0.0.0",
  "description": "ClanPress is full featured clan management plugin.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "minify:css": "cleancss -o dist/css/backend.min.css dist/css/backend.css",
    "minify:js": "uglifyjs --compress --mangle -- dist/js/teamspeak-viewer.js > dist/js/teamspeak-viewer.min.js",
    "minify": "npm run minify:js && npm run minify:css",
    "compile:css": "node-sass src/scss/backend.scss > dist/css/backend.css",
    "compile:js": "babel src/es6 -d dist/js",
    "compile": "npm run compile:js && npm run compile:css && npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aureolacodes/clanpress.git"
  },
  "author": "Christian Hanne <support@aureola.codes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aureolacodes/clanpress/issues"
  },
  "homepage": "https://github.com/aureolacodes/clanpress#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "clean-css": "^3.4.9",
    "node-sass": "^3.4.2",
    "uglify-js": "^2.6.1"
  }
}
