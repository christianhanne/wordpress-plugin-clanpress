{
  "name": "clanpress",
  "version": "0.0.0",
  "description": "ClanPress is full featured clan management plugin.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "compile:minify": "cat src/css/*.css | minify --css && cat src/js/*.js | minify --js",
    "compile:css": "node-sass src/scss/backend.scss > dist/css/backend.css",
    "compile:js": "babel src/es6 -d dist/js",
    "compile": "npm run compile:js && npm run compile:css && npm run compile:minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aureolacodes/clanpress.git"
  },
  "author": "Christian Hanne <support@aureola.codes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aureolacodes/clanpress/issues"
  },
  "homepage": "https://github.com/aureolacodes/clanpress#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "minify": "^2.0.3",
    "node-sass": "^3.4.2"
  }
}
